{
  "metadata": [
    {
      "src": [
        {
          "src": "../../../",
          "files": [
            "PhantasmicDev.PackageTemplate.csproj",
            "PhantasmicDev.PackageTemplate.Editor.csproj"
          ]
        }
      ],
      "dest": "api",
      "filter": "filterConfig.yml"
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "toc.yml",
          "index.md",
          "changelog.md"
        ]
      },
      {
        "src": "../",
        "files": [
          "LICENSE.md",
          "Third Party Notices.md"
        ],
        "dest": "license"
      },
      {
        "src": "../",
        "files": [
          "CHANGELOG.md"
        ]
      },
      {
        "src": "manual",
        "files": [
          "toc.yml",
          "*.md"
        ],
        "dest": "manual"
      },
      {
        "src": "api",
        "files": [
          "toc.yml",
          "*.yml",
          "*.md"
        ],
        "dest": "api"
      },
      {
        "src": "license",
        "files": [
          "toc.yml",
          "*.md"
        ],
        "dest": "license"
      }
    ],
    "resource": [
      "resources/**"
    ],
    "globalMetadata": {
      "_appTitle": "Package Template",
      "_appFooter": "<span>Copyright © PhantasmicDev <br>Generated with <strong>DocFX</strong></span>",
      "_enableSearch": true
    },
    "template": [
      "default",
      "templates/upm-package-fx",
      "templates/phantasmic-fx"
    ],
    "xref": [
      "https://normanderwan.github.io/UnityXrefMaps/xrefmap.yml"
    ],
    "dest": "_site"
  }
}